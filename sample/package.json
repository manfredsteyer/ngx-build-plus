{
  "name": "elements-loading",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve --extraWebpackConfig webpack.serve.extra.js -o --single-bundle true",
    "build": "ng build --prod --extraWebpackConfig webpack.extra.js --output-hashing none --single-bundle true",
    "start:prod": "live-server dist/elements-loading",
    "copy:bundles": "node copy-bundles.js",
    "postinstall": "npm run copy:bundles",
    "build:modern": "ng build --plugin ngx-build-modern --prod",
    "build:modern:elements-loading": "ng build --plugin ngx-build-modern --project elements-loading --prod",
    "build:modern:empty-demo": "ng build --plugin ngx-build-modern --project empty-demo --prod",
    "build:plugin": "tsc hook/modern-plugin --module commonjs --target es5 --lib DOM,ES6,ScriptHost && ng build --plugin ~hook/modern-plugin",
    "build:test": "tsc test.ts --module commonjs --target es5 --lib DOM,ES6,ScriptHost && node test.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/elements": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@webcomponents/custom-elements": "^1.1.2",
    "core-js": "^2.5.4",
    "document-register-element": "^1.7.2",
    "domino": "^2.1.0",
    "ngx-build-modern": "^0.9.2",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "^7.0.2",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "copy": "^0.3.2",
    "cpr": "^3.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "live-server": "^1.2.0",
    "ngx-build-plus": "^7.1.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.1.3",
    "webpack-cli": "^3.0.3"
  }
}
